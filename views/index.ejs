<script src="http://mpa-tiles.craigmmills.com/javascripts/polymaps.js" type="text/javascript"></script>

<style type="text/css">
	.layer path {
	  fill: #760B10;
	  stroke: #FFDFA6;
	  fill-opacity: .7;
	}
	
</style>


<div id="map" style="width: 100%; height: 600px"></div>
<script type="text/javascript">
  var po = org.polymaps;

var map = po.map()
    .container(document.getElementById("map").appendChild(po.svg("svg")))
    .add(po.interact())
    .add(po.hash());

map.add(po.image()
    .url(po.url("http://{S}tile.cloudmade.com"
    + "/1a1b06b230af4efdbb989ea99e9841af" // http://cloudmade.com/register
    + "/998/256/{Z}/{X}/{Y}.png")
    .hosts(["a.", "b.", "c.", ""])));

map.add(po.compass()
    .pan("none"));

	var layer;
	layer = po.geoJson().url("http://geoipsum.org/polygons.json?perimeter=594&bearing_range=70&polygon_number=30&bb=40.4154619,-129.7709961,60.2192732,-7.3393555");
	map.add(layer);

</script>




